<HEADER state="1">
<Title>Create json of type key:value</Title>
<ArchiveKey1/>
<ArchiveKey2/>
<OH_SubType/>
</HEADER>
<SCRIPT mode="1" state="1">
<MSCRI><![CDATA[:SET &JSON_OBJ# = STR_CAT('"&UC4RB_FIELD_NAME#":',&UC4RB_FIELD_VALUE#)
